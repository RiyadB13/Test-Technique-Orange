      "Strasbourg           0\n",
      "Name: is_outlier, dtype: int64,)\n"
     ]
    }
   ],
   "source": [
    "# Fonction pour détecter les outliers dans une colonne donnée\n",
    "def detect_outliers_by_city(data, column, group_column):\n",
    "    # Calcul des bornes IQR pour détecter les outliers\n",
    "    Q1 = data[column].quantile(0.25)\n",
    "    Q3 = data[column].quantile(0.75)\n",
    "    IQR = Q3 - Q1\n",
    "    lower_bound = Q1 - 1.5 * IQR\n",
    "    upper_bound = Q3 + 1.5 * IQR\n",
    "    \n",
    "    # Ajouter une colonne indiquant si une ligne est un outlier\n",
    "    data['is_outlier'] = (data[column] < lower_bound) | (data[column] > upper_bound)\n",
    "    \n",
    "    # Compter les outliers par ville (ou autre colonne de regroupement)\n",
    "    outliers_by_city = data.groupby(group_column)['is_outlier'].sum()\n",
    "    \n",
    "    return outliers_by_city\n",
    "\n",
    "# Exemple d'application sur les revenus du jPhone Pro\n",
    "outliers_jphone_by_city = detect_outliers_by_city(sales_data, 'jPhone_Pro_revenue', 'city')\n",
    "outliers_Kaggle_by_city = detect_outliers_by_city(sales_data, 'Kaggle_Pixel_5_revenue', 'city')\n",
    "outliers_Planet_by_city = detect_outliers_by_city(sales_data, 'Planet_SX_revenue', 'city'), \n",
    "\n",
    "# Afficher les résultats\n",
    "print(\"Nombre d'outliers par ville pour chaque modele :\")\n",
    "print('jPhone_Pro :\\n', outliers_jphone_by_city)\n",
    "print('Kaggle_Pixel_5:\\n', outliers_Kaggle_by_city)\n",
    "print('Planet_SX:\\n', outliers_Planet_by_city)"
   ]
  },
